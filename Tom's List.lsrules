{
  "name" : "Tom's List",
  "description" : "Specific apps and some ad domains",
  "rules" : [
    {
      "action" : "deny",
      "ports" : "6698-6699",
      "process" : "any",
      "remote" : "any"
    },
    
    {
      "action" : "deny",
      "process" : "\/usr\/sbin\/ocspd",
      "remote" : "any"
    },
    
    {
      "action" : "deny",
      "process" : "\/Applications\/Utilities\/Adobe Application Manager\/UWA\/AAM Updates Notifier.app\/Contents\/MacOS\/AAM Updates Notifier",
      "remote-domains" : "adobe.com"
    },

    {
      "action" : "deny",
      "process" : "\/Applications\/MAMP\/MAMP.app\/Contents\/MacOS\/MAMP",
      "remote-domains" : "mamp.info"
    },

    {
      "action" : "deny",
      "process" : "\/Applications\/Maps.app\/Contents\/MacOS\/Maps",
      "remote-domains" : "facebook.com"
    },

    {
      "action" : "deny",
      "process" : "\/Applications\/Microsoft Word.app\/Contents\/MacOS\/Microsoft Word",
      "remote-domains" : "cityofglacol.ac.uk"
    },

    {
      "action" : "deny",
      "process" : "\/Library\/Application Support\/Microsoft\/MAU2.0\/Microsoft AutoUpdate.app\/Contents\/MacOS\/Microsoft Update Assistant.app\/Contents\/MacOS\/Microsoft Update Assistant",
      "remote-domains" : [
        "akamaized.net",
        "azurewebsites.net",
        "microsoft.com"
      ]
    },
    
    {
      "action" : "deny",
      "process" : "\/System/Library/PrivateFrameworks/PhotoAnalysis.framework/Versions/A/Support/photoanalysisd",
      "remote" : "any"
     },
     
    {
      "action" : "deny",
      "process" : "\/Applications\/Microsoft Excel.app\/Contents\/SharedSupport\/Microsoft Error Reporting.app\/Contents\/MacOS\/Microsoft Error Reporting",
      "remote" : "any"
    },

    {
      "action" : "allow",
      "process" :  "\/Applications\/Microsoft Word.app\/Contents\/MacOS\/Microsoft Word",
      "remote-domains" : [
        "live.com",
        "microsoft.com",
        "microsoftonline.com",
        "msauth.net",
        "msedge.net",
        "oaspapps.com",
        "office.com",
        "office365.com",
        "office.net",
        "windows.net"
      ]
    },
    
    {
      "action" : "allow",
      "process" : "\/Applications\/Microsoft PowerPoint.app\/Contents\/MacOS\/Microsoft PowerPoint",
      "remote-domains" : [
        "live.com",
